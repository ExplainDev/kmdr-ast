{
  "name": "kmdr-ast",
  "version": "2.0.2",
  "description": "Traverse the AST of an explanation by kmdr",
  "main": "dist/index.js",
  "repository": "git@github.com:ediardo/kmdr-ast.git",
  "author": "Eddie Ramirez <djedi.r@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.13",
    "@types/react-test-renderer": "^16.9.1",
    "jest": "^24.9.0",
    "kmdr-parser": "^1.1.3",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose",
    "publish": "yarn run build && np --no-tests"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/",
      "build/",
      "src/",
      "node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.(ts|tsx|js)$",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            2345
          ]
        }
      }
    }
  }
}
